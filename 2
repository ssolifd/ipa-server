name=ipa
cat > docker-compose.yml<<-EOF
version: "2"

services:
  web:
    #build: .
    container_name: solidfd/ipaserver_web
    restart: always
    environment:
      - NODE_ENV=production
      - PUBLIC_URL=https://$name.godoq.com
    ports:
      - "9008:8080"
    volumes:
      - "/docker/data/ipa-server:/app/upload"
EOF
echo "https://$name.godoq.com {
gzip
tls en@qq.com
 proxy / localhost:9008
}" > /usr/local/caddy/Caddyfile
service caddy restart
